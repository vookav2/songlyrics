{
  "name": "songlyrics",
  "version": "2.3.3",
  "description": "Find a lyrics just enter the song title",
  "keywords": [
    "lyrics",
    "songlyrics",
    "unofficial"
  ],
  "scripts": {
    "ts:watch": "ts-node-dev --respawn --transpile-only --poll ./src/index.ts",
    "ts:build": "rimraf ./build && tsc",
    "build": "rimraf ./build && tsc",
    "pretty": "prettier --write .",
    "lint": "eslint . --ext .ts",
    "prepare": "husky install && yarn build"
  },
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "files": [
    "/build"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vookav2/songlyrics"
  },
  "author": "mr687 <davinomoehdanino@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "cheerio": "^1.0.0-rc.11",
    "duck-duck-scrape": "^2.2.1",
    "got": "^11.8.2",
    "user-agents": "^1.0.793"
  },
  "devDependencies": {
    "@types/node": "^16.10.2",
    "@types/user-agents": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.1.8",
    "tsc-files": "^1.1.3",
    "typescript": "^4.4.3"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "yarn eslint",
      "yarn prettier --write"
    ],
    "**/*.ts": [
      "tsc-files --noEmit"
    ]
  }
}
